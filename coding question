// Online C compiler to run C program online
#include <stdio.h>
#include<stdlib.h>
int diffPossible(int* A, int n1, int B) {
    for(int i=0;i<n1;i++)
    {
        for(int j=i+1;j<n1;j++){
            
        
        int diff=A[i]-A[j];
        int diff1=abs(diff);
       
        if (diff1 == B){
            return 1;
        }
    
        }
      
        }
            return 0;
    }


int main() {
    // Write C code here

 int   arr[]={1,3,4
 };
   int result= diffPossible(arr,3,4);
   printf("%d",result);
   

}
